# Production Environment Variables
# Copy this file to .env.production and replace with your actual production API keys

# AssemblyAI Speech Recognition API
VITE_ASSEMBLYAI_API_KEY=your_assemblyai_production_key

# Reverie Language AI API (for Indian regional languages)
VITE_REVERIE_API_ID=your_reverie_production_id
VITE_REVERIE_API_KEY=your_reverie_production_key

# Google Cloud Speech-to-Text API (optional)
VITE_GOOGLE_CLOUD_API_KEY=your_google_production_key

# Azure Cognitive Services API (optional)
VITE_AZURE_SPEECH_KEY=your_azure_production_key
VITE_AZURE_SPEECH_REGION=your_azure_region

# Application Settings
VITE_APP_NAME=ClinIQ
VITE_APP_ENVIRONMENT=production
VITE_APP_VERSION=1.0.0

# Security Settings
VITE_ENABLE_HTTPS=true
VITE_ENABLE_CORS=true
